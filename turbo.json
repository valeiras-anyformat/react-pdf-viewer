{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "js": {
            "cache": false,
            "dependsOn": ["^js"]
        },
        "css": {
            "cache": false,
            "dependsOn": ["^css"]
        },
        "types": {
            "cache": false,
            "dependsOn": ["^types"]
        },
        "release": {
            "dependsOn": ["^release"]
        },
        "test": {
            "dependsOn": ["^test"]
        }
    }
}
